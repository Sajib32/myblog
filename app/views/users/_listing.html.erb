<div class="listing">
		<table>
			<tr>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Username</th>
				<th>User Level</th>
			</tr>
		  	  <% for user in @users %>
			   <tr class="<%= cycle('row1', 'row2') %>">
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.username %></td>
				<td><%= user.user_level %></td>
				<td><%= link_to('Edit', :action => 'manage', :id => user) %></td>
			   </tr>
			   <% end %>
		</table>
	</div>
